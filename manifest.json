{
  "manifest_version": 2,

  "name": "Extensionary",
  "description": "A vocabulary builder extension",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ],
  "version": "1.0",
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_popup": "content.html"
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}
